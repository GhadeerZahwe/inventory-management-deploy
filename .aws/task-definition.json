{
  "family": "inventory-task",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "arn:aws:iam::159246961426:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "inventory-container",
      "image": "159246961426.dkr.ecr.us-east-1.amazonaws.com/inventory-app@sha256:21f6bafd186d3aa2ca4cfb75df96eba11e748f66e301f81bc6ec540d74768fc9",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http",
          "name": "inventory-container-80-tcp"
        }
      ],
      "cpu": 0,
      "environment": [
        {
          "name": "awslogs-group",
          "value": "/ecs/inventory"
        },
        {
          "name": "awslogs-create-group",
          "value": "true"
        },
        {
          "name": "awslogs-region",
          "value": "us-east-1"
        },
        {
          "name": "awslogs-stream-prefix",
          "value": "ecs"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/inventory-task",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "mountPoints": [],
      "ulimits": [],
      "volumesFrom": []
    }
  ],
  "placementConstraints": [],
  "volumes": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
