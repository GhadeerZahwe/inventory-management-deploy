{
  "family": "inventory-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "inventory-container",
      "image": "REPLACE_WITH_IMAGE_URI",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
